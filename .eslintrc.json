{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "semi": [
            "error",
            "always"
        ],
        "semi-style": "error",
        "no-extra-semi": "error",
        "no-invalid-regexp": "error",
        "eqeqeq": "error",
        "use-isnan": "error",
        "getter-return": "error",
        "valid-typeof": "error",
        "no-extra-bind": "error",
        "no-eq-null": "error",
        "no-labels": "error",
        "no-multi-spaces": "error",
        "no-multi-str": "error",
        "no-new": "error",
        "no-new-func": "error",
        "no-new-wrappers": "error",
        "no-redeclare": "error",
        "array-bracket-newline": [
            "off",
            "always"
        ],
        "array-element-newline": [
            "off"
        ],
        "block-spacing": [
            "error",
            "always"
        ],
        "brace-style": "error",
        "camelcase": "off",
        "comma-dangle": "off",
        "comma-spacing": "error",
        "func-call-spacing": [
            "error",
            "never"
        ],
        "lines-between-class-members": "off",
        "max-len": [
            "error",
            {
                "code": 1000
            }
        ],
        "max-lines": [
            "error",
            1000
        ],
        "no-new-object": "error",
        "semi-spacing": "error",
        "arrow-spacing": "error",
        "arrow-parens": "error",
        "constructor-super": "error",
        "no-duplicate-imports": "error",
        "no-var": "error",
        "no-unused-vars": "warn",
        "indent": "off",
        "no-shadow": "off",
        "import/prefer-default-export": "off",
        "import/extensions": "off",
        "import/no-unresolved": "off",
        "quotes": [
            "error",
            "double"
        ],
        "no-restricted-syntax": "off",
        "no-param-reassign": "off",
        "no-continue": "off",
        "class-methods-use-this": "off",
        "no-use-before-define": "off",
        "no-void": "off",
        "no-unused-expressions": "off",
        "no-return-await": "off",
        "key-spacing": "off",
        "curly": "off",
        "nonblock-statement-body-position": "off",
        "prefer-const": "off",
        "no-plusplus": "off",
        "no-return-assign": "off",
        "no-bitwise": "off",
        "operator-linebreak": "off",
        "object-curly-newline": "off",
        "no-undef": "off",
        "no-dupe-class-members": "off"
    }
}